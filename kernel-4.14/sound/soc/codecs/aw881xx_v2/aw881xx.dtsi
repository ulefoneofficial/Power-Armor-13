/* AWINIC AW881XX Smart PA */
&i2c3 {
	aw881xx_smartpa@34 {
		compatible = "awinic,aw881xx_smartpa";
		reg = <0x34>;
		reset-gpio = <&pio 65 0>;
		irq-gpio = <&pio 5 0>;
		monitor-flag = <1>;
		fade-enable = <0>;
        pa-syn-enable = <0>;
		status = "okay";
	};
};
/* AWINIC AW881XX Smart PA End */
