&i2c1 {
/* AWINIC AW87XXX Smart K PA */
	aw87xxx_pa_58@58 {
		compatible = "awinic,aw87xxx_pa";
		reg = <0x58>;
		reset-gpio = <&pio 63 0>;
		pa-channel = < 0 >;
		status = "okay";
	};

	aw87xxx_pa_5b@5b {
		compatible = "awinic,aw87xxx_pa";
		reg = <0x5b>;
		reset-gpio = <&pio 1 0>;
		pa-channel = < 1 >;
		status = "okay";
	};


/* AWINIC AW87XXX Smart K PA End */
};
